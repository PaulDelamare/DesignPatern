# Configuration environnementale de l'API designpatern
# Dupliquer ce fichier en .env et ajuster les valeurs ci-dessous.

# Chaîne de connexion PostgreSQL complète
DATABASE_URL="postgresql://user:password@localhost:5432/security_app?schema=public"

# Paramètres HTTP
PORT=3000
API_URL="http://localhost:3000"
API_KEY="dev-shared-key"

# Secrets JWT
JWT_SECRET="change-me-with-a-strong-random-string"
